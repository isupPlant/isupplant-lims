<resources>


    <style name="InspectionApplicationFilterStyle">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center</item>
        <item name="android:button">@null</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">@drawable/sl_theme_blue_text</item>
    </style>

    <style name="yh_custom_view_horizontal_style">
        <item name="android:paddingStart">@dimen/dp_15</item>
        <item name="android:textSize">@dimen/dp_16</item>
        <item name="bold">false</item>
        <item name="android:background">@color/white</item>
        <item name="android:layout_width">match_parent</item>
        <item name="edit_maxLine">5</item>
        <item name="text_width">@dimen/dp_120</item>
        <item name="android:paddingRight">@dimen/dp_10</item>
        <item name="android:layout_height">46dp</item>
        <item name="key_width">@dimen/dp_120</item>
    </style>
    <style name="FilterStyleDate">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_marginLeft">@dimen/dp_10</item>
        <item name="android:layout_marginRight">@dimen/dp_10</item>
        <item name="android:button">@null</item>
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">@color/sl_blue_text</item>
    </style>
</resources>
