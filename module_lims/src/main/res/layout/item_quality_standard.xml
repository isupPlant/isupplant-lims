<?xml version="1.0" encoding="utf-8"?>
<com.supcon.common.view.view.CustomSwipeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/itemSwipeLayout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:tag="itemSwipeLayout"
    android:background="@color/white"
    tools:ignore="MissingDefaultResource">
<LinearLayout
    android:id="@+id/item"
    android:tag="item"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:orientation="vertical"
    android:paddingStart="@dimen/dp_10"
    android:paddingTop="@dimen/dp_10"
    android:paddingEnd="@dimen/dp_15"
    android:paddingBottom="@dimen/dp_10">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_35"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_quality_standard" />

        <com.supcon.mes.mbap.view.CustomTextView
            android:id="@+id/ctQualityStandard"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginStart="@dimen/dp_10"
            android:tag="ctQualityStandard"
            app:content_color="@color/dark_text"
            app:content_size="14"
            app:gravity="right|center_vertical"
            app:key="@string/lims_quality_standard"
            app:key_color="@color/c_scan_666"
            app:key_size="14" />


    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_35"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_executive_standards" />

        <com.supcon.mes.mbap.view.CustomTextView
            android:id="@+id/ctStandards"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginStart="@dimen/dp_10"
            android:tag="ctStandards"
            app:content_color="@color/dark_text"
            app:content_size="14"
            app:gravity="right|center_vertical"
            app:key="@string/lims_executive_standards"
            app:key_color="@color/c_scan_666"
            app:key_size="14" />


    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_35"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_version" />

        <com.supcon.mes.mbap.view.CustomTextView
            android:id="@+id/ctVersionNumber"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginStart="@dimen/dp_10"
            android:tag="ctVersionNumber"
            app:content_color="@color/dark_text"
            app:content_size="14"
            app:gravity="right|center_vertical"
            app:key="@string/lims_version_number"
            app:key_color="@color/c_scan_666"
            app:key_size="14" />


    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_35"
        android:gravity="center_vertical"
        android:orientation="horizontal">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_programme" />

        <com.supcon.mes.mbap.view.CustomTextView
            android:id="@+id/ctApplicationScheme"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginLeft="@dimen/dp_10"
            android:tag="ctApplicationScheme"
            app:content_color="@color/dark_text"
            app:content_size="14"
            app:editable="true"
            app:gravity="center|center_vertical"
            app:key="@string/lims_application_scheme"
            app:key_color="@color/c_scan_666"
            app:key_size="14" />


    </LinearLayout>


</LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/itemViewDelBtn"
            android:layout_width="@dimen/dp_80"
            android:layout_height="match_parent"
            android:background="@color/deleteBgColor"
            android:gravity="center"
            android:tag="itemViewDelBtn"
            android:text="删除"
            android:textColor="@color/white"
            android:textSize="@dimen/fontSize_14sp" />
    </LinearLayout>
</com.supcon.common.view.view.CustomSwipeLayout>