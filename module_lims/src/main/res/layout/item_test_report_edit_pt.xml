<?xml version="1.0" encoding="utf-8"?>
<com.supcon.common.view.view.CustomSwipeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/itemSwipeLayout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:tag="itemSwipeLayout"
    tools:ignore="MissingDefaultResource">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/white"
            android:orientation="vertical"
            android:paddingStart="@dimen/dp_10"
            android:paddingTop="@dimen/dp_10"
            android:paddingEnd="@dimen/dp_15"
            >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_35"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_inspect_project" />

                <com.supcon.mes.mbap.view.CustomTextView
                    android:id="@+id/ctTestProject"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="@dimen/dp_10"
                    android:tag="ctTestProject"
                    app:content_color="@color/dark_text"
                    app:content_size="14"
                    app:gravity="right|center_vertical"
                    app:key="@string/lims_report_name"
                    app:key_color="@color/c_scan_666"
                    app:key_size="14" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/llCeDispValue"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_35"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:tag="llCeDispValue">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_inspect_dispvalue" />

                <com.supcon.mes.mbap.view.CustomEditText
                    android:id="@+id/ceDispValue"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="@dimen/dp_10"
                    android:tag="ceDispValue"
                    app:content_color="@color/dark_text"
                    app:content_size="14"
                    app:gravity="right|center_vertical"
                    app:key="@string/lims_dispvalue"
                    app:key_size="14" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/llEnumDispValue"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_35"
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:tag="llEnumDispValue">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_inspect_dispvalue" />

                <com.supcon.mes.mbap.view.CustomSpinner
                    android:id="@+id/csDispValue"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="@dimen/dp_10"
                    android:tag="csDispValue"
                    app:content_color="@color/dark_text"
                    app:content_size="14"
                    app:gravity="right|center_vertical"
                    app:key="@string/lims_dispvalue"
                    app:key_size="14" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_35"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/ic_inspect_check_result" />

                <com.supcon.mes.mbap.view.CustomSpinner
                    android:id="@+id/csTestConclusion"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginStart="@dimen/dp_10"
                    android:tag="csTestConclusion"
                    app:content_color="@color/dark_text"
                    app:content_size="14"
                    app:gravity="right|center_vertical"
                    app:key="@string/lims_check_result"
                    app:key_color="@color/c_scan_666"
                    app:key_size="14" />

            </LinearLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_35"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/inspectRangeImg"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:src="@drawable/ic_inspect_range" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/dp_10"
                    android:layout_toRightOf="@+id/inspectRangeImg"
                    android:gravity="center"
                    android:text="@string/lims_range" />

                <ImageView
                    android:id="@+id/rangeImg"
                    android:layout_width="@dimen/dp_30"
                    android:layout_height="@dimen/dp_30"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:scaleType="centerInside"
                    android:src="@drawable/ic_inspect_up_arrow"
                    android:tag="rangeImg" />
            </RelativeLayout>
        </LinearLayout>

        <View
            android:id="@+id/endView"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_5"
            android:background="#fff8f8f8"
            android:tag="endView" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/itemViewDelBtn"
            android:layout_width="@dimen/dp_80"
            android:layout_height="match_parent"
            android:background="@color/deleteBgColor"
            android:gravity="center"
            android:tag="itemViewDelBtn"
            android:text="@string/lims_delete"
            android:textColor="@color/white"
            android:textSize="@dimen/fontSize_14sp" />
    </LinearLayout>
</com.supcon.common.view.view.CustomSwipeLayout>