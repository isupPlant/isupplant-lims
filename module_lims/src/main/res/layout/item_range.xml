<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="@dimen/dp_35"
    android:background="#FFF0F4FC"
    android:orientation="vertical"
    android:paddingStart="@dimen/dp_10"
    android:paddingTop="@dimen/dp_10"
    android:paddingEnd="@dimen/dp_15"
    android:paddingBottom="@dimen/dp_10">

    <com.supcon.mes.mbap.view.CustomTextView
        android:id="@+id/judgeRangeTv"
        android:tag="judgeRangeTv"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:content_color="@color/dark_text"
        app:content_size="14"
        app:gravity="right|center_vertical"
        app:key="@string/lims_report_name"
        app:key_color="@color/c_scan_666"
        app:key_size="14" />


</LinearLayout>